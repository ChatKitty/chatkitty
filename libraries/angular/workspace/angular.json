{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@chatkitty/angular": {
      "projectType": "library",
      "root": "projects/chatkitty",
      "sourceRoot": "projects/chatkitty/src",
      "prefix": "library",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/chatkitty/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/chatkitty/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/chatkitty/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/chatkitty/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "f78da42c-94d1-40f8-8dd7-911cc4af3530"
  }
}
